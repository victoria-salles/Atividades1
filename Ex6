//Desenvolver um  algoritmo  que  leia a  altura  de  um  homem 
        //e  calcule  o  seu  peso  ideal, utilize  a seguinte fórmula: 
        //PESO IDEAL = (72.7 * H) –58, onde H = altura.

        double altura, pesoIdeal;

        public void print()
        {

            Console.WriteLine("Altura:");
            altura =double.Parse(Console.ReadLine());
            pesoIdeal = (72.7 * altura) - 58;
            Console.WriteLine($"O 'Peso Ideal' masculino é: {pesoIdeal}");
        }
