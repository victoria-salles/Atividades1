//Desenvolva um algoritmo para converter a temperatura em graus Celsius para Fahrenheit,
//a fórmula é F = (9 * C + 160) / 5;

Console.WriteLine("Temperatura em celsius:");
string celsius = Console.ReadLine();
double fahrenheit = (9 * Convert.ToDouble(celsius) + 160) / 5;
Console.WriteLine("Temperatura em Fahrenheit:");
Console.WriteLine(fahrenheit);
Console.ReadLine();
